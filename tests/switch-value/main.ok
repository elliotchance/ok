func main() {
    a = 1

    print("before switch 1")
    switch a * 2 {
        case 2 {
            print("TWO")
        }
        case 4 {
            print("FOUR")
        }
        case 6 {
            print("SIX")
        }
    }
    print("after switch 1")

    print("before switch 1a")
    switch a * 2 {
        case 2, 4 {
            print("TWO OR FOUR")
        }
        case 6 {
            print("SIX")
        }
    }
    print("after switch 1a")

    print("before switch 2")
    ++a
    switch a * 2 {
        case 2 {
            print("TWO")
        }
        case 4 {
            print("FOUR")
        }
        case 5 {
            print("SIX")
        }
        else {
            print("ELSE")
        }
    }
    print("after switch 2")

    print("before switch 2a")
    switch a * 2 {
        case 2, 4 {
            print("TWO OR FOUR")
        }
        case 6 {
            print("SIX")
        }
        else {
            print("ELSE")
        }
    }
    print("after switch 2a")

    print("before switch 3")
    a += 2
    switch a * 2 {
        case 2 {
            print("TWO")
        }
        case 4 {
            print("FOUR")
        }
        case 6 {
            print("SIX")
        }
        else {
            print("ELSE")
        }
    }
    print("after switch 3")

    print("before switch 3a")
    switch a * 2 {
        case 2, 4 {
            print("TWO OR FOUR")
        }
        else {
            print("ELSE")
        }
    }
    print("after switch 3a")
}
