func main() {
    fn = func (a number) number {
        return a * 2
    }

    print(fn(7.3))

    A = 12
    b = 3.7

    fn = func (a, b number) number {
        return a * b + ^A + ^b
    }

    print(fn(3, 5))

    b = 5.7

    print(fn(3, 5))

    func setA(newVal number) {
        ^A = newVal
        print(^A)
    }

    setA(7.8)
    print(fn(3, 5))
}
