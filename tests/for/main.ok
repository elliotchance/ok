func main() {
    i = 0
    for i < 5 {
        print(i)
        i = i + 1
    }

    print("before for 1")
    for {
        print("before break")
        break
        print("after break")
    }
    print("after for")

    print("before for 2")
    i = 0
    for i < 3 {
        print("before continue", i)
        i = i + 1
        continue
        print("after continue")
    }
    print("after for")
}
